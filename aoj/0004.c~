#include <stdio.h>

int main()
{
  double a, b, c, d, e, f;
  double x, y;
  while(scanf("%f %f %f %f %f %f", a, b, c, d, e, f)!= EOF)
    {
      y = (c * a - f * d) / ( b * a - e * d ) + 0.0005;
      x = (c * b - f * e) / ( a * b - d * e ) + 0.0005;
      printf("%.3f %.3f\n", x, y);
    }
  return 0;
}
      
